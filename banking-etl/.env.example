# ============================================================================
# Banking ETL Pipeline - .env Example File
# ============================================================================
# Copy this file to .env and fill in your actual values.
# NEVER commit .env to version control!

# ============================================================================
# PostgreSQL Configuration
# ============================================================================
POSTGRES_USER=airflow
POSTGRES_PASSWORD=123
POSTGRES_DB=banking_warehouse
POSTGRES_HOST=postgres
POSTGRES_PORT=5432
DATABASE_URL=postgresql://airflow:123@postgres:5432/banking_warehouse

# ============================================================================
# Airflow Configuration
# ============================================================================
AIRFLOW_HOME=/home/airflow/airflow
AIRFLOW__CORE__SQL_ALCHEMY_CONN=postgresql+psycopg2://airflow:airflow@airflow-db/airflow
AIRFLOW__CORE__FERNET_KEY=MDEyMzQ1Njc4OWFiY2RlZjAxMjM0NTY3ODlhYmNkZWY=
ALERT_EMAIL=
AIRFLOW_UID=50000

# ============================================================================
# ETL Configuration
# ============================================================================
DATA_INPUT_PATH=/data/Comprehensive_Banking_Database.csv.xlsx
DW_SCHEMA_NAME=banking_dw
STAGING_SCHEMA_NAME=staging
LOG_LEVEL=INFO

# ============================================================================
# Data Quality Configuration
# ============================================================================
MIN_COMPLETENESS_PCT=95
MAX_NULL_PCT=5
DUPLICATE_CHECK_ENABLED=True

# ============================================================================
# PgAdmin Configuration
# ============================================================================
PGADMIN_DEFAULT_EMAIL=admin@example.com
PGADMIN_DEFAULT_PASSWORD=admin_password_here
